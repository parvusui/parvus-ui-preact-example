    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "5.1.5",
    "eslint": "4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-preact": "^1.1.6",
